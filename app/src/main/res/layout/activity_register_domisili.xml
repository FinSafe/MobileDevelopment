<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.register.RegisterDomisiliActivity">

    <TextView
        android:id="@+id/tvProvinsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Provinsi domisili"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@+id/provinsi_domisili"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/provinsi_domisili"
        style="@style/Widget.Material3.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvProvinsi">

        <AutoCompleteTextView
            android:id="@+id/autoCompletedomisili"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:inputType="none"/>
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/tvLuasRumah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Luas rumah"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/LuasRumah"
        app:layout_constraintTop_toBottomOf="@+id/provinsi_domisili" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/LuasRumah"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvLuasRumah">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/nameEditText"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:inputType="text"
            android:hint="contoh: 178"/>
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/tvTipe_bangunan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Tipe bangunan rumahh"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/LuasRumah"
        app:layout_constraintTop_toBottomOf="@+id/LuasRumah" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/Tipe_bangunan"
        style="@style/Widget.Material3.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvTipe_bangunan">

        <AutoCompleteTextView
            android:id="@+id/autoCompletebangunan"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:inputType="none"/>
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/tvListrik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="listrik"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/Listrik"
        app:layout_constraintTop_toBottomOf="@+id/Tipe_bangunan" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/Listrik"
        style="@style/Widget.Material3.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvListrik">

        <AutoCompleteTextView
            android:id="@+id/autoCompletelistik"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:inputType="none"/>
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/tvKamar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="banyaknya kamar tidur"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/Kamar"
        app:layout_constraintTop_toBottomOf="@+id/Listrik" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/Kamar"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvKamar">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/texeditTP"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:hint="contoh: 5"
            android:inputType="text" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/tvStatus_milik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Status kepemilikan"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/Status_milik"
        app:layout_constraintTop_toBottomOf="@+id/Kamar" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/Status_milik"
        style="@style/Widget.Material3.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvStatus_milik">

        <AutoCompleteTextView
            android:id="@+id/autoCompletekepemilikan"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:inputType="none"/>
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/registerButton"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/purple"
        android:text="@string/register2"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/Status_milik"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>